{
   "name": "Baixa Arquivos",
   "version": "1.0",
   "description": "Baixa links de uma p√°gina",
   "manifest_version": 2,
   "permissions": [ 
      "activeTab",  
      "downloads" 
   ],
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "jquery-2.1.4.min.js", "content.js" ],
      "matches": [ "<all_urls>" ]
   } ],
    "browser_action": {
       "default_popup": "index.html",
       "default_icon":"Logotipo.png",
       "default_title": "Baixa Arquivos"
    }
 }